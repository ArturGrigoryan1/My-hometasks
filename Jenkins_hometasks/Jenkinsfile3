pipeline {
    agent any
    triggers {
    GenericTrigger(
        genericVariables: [
            [defaultValue: '', key: 'base', regexpFilter: '', value: '$.ref'],
            ],
     causeString: 'Triggered on $base',
     token: 'test',
     tokenCredentialId: '' )
  }
    stages {
        stage('Check branch name') {
            steps {
                echo 'Hello world'
                echo env.base
            }
        }
        
}
}
